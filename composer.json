{
    "name": "despark/html-template-curator",
    "description": "Laravel 4 package, which enables you to manage complicated HTML templates, preserving the design integrity.",
	"keywords": ["laravel","rich", "text", "editor", "wysiwyg", "despark"],
	"license": "MIT",
	"type": "plugin",
    "authors": [
        {
            "name": "Stoyan Dipchikov",
            "email": "sdipchikov@despark.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "intervention/image": "~2",
        "intervention/imagecache": "~2.1",
        "cviebrock/image-validator": "1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/Despark"
        ],
        "psr-0": {
            "Despark\\HtmlTemplateCurator\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
